msgid "Graphical Menu Bar"
msgstr "របារ​ម៉ឺនុយ​ក្រាហ្វ៊ិក"

msgid "Type of Images"
msgstr "ប្រភេទ​រូបភាព"

msgid "None (text only)"
msgstr "គ្មាន (តែ​អត្ថបទ​ប៉ុណ្ណោះ)"

msgid "Icons"
msgstr "រូបតំណាង"

msgid "Icons with Text"
msgstr "រូបតំណាង​មាន​អត្ថបទ"

msgid "Buttons"
msgstr "ប៊ូតុង"

msgid "Text alignment for <b>Icons with Text</b>"
msgstr "ការ​តម្រឹម​អត្ថបទ​សម្រាប់​<b>រូបតំណាង​មាន​អត្ថបទ</b>"

msgid "After icon"
msgstr "នៅ​បន្ទាប់​រូបតំណាង"

msgid "Below icon"
msgstr "នៅ​ក្រោម​រូបតំណាង"

msgid "Text Size"
msgstr "ទំហំ​អត្ថបទ"

msgid "Default"
msgstr "លំនាំដើម"

msgid "Small"
msgstr "តូច"

msgid "Medium"
msgstr "មធ្យម"

msgid "Large"
msgstr "ធំ"

msgid "Cache Buttons locally"
msgstr "ប៊ូតុង​ឃ្លាំង​សម្ងាត់​មូលដ្ឋាន"

