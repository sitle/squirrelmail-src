msgid "Add New User Permission"
msgstr "បន្ថែម​សិទ្ធិ​អ្នកប្រើ​ថ្មី"

msgid "Add New Share to User"
msgstr "បន្ថែម​ការ​ចែក​រំលែក​ថ្មី​ទៅ​កាន់​អ្នកប្រើ"

msgid "Close"
msgstr "បិទ"

msgid "Read"
msgstr "អាន"

msgid "Insert"
msgstr "បញ្ចូល"

msgid "Post"
msgstr "ប្រកាស"

msgid "Delete"
msgstr "លុប"

msgid "Admin"
msgstr "អ្នក​គ្រប់គ្រង"

msgid "Folder"
msgstr "ថត"

msgid "User"
msgstr "អ្នកប្រើ"

msgid "Fill in a username, &quot;anyone&quot; or &quot;anonymous&quot;"
msgstr "បំពេញ​ឈ្មោះ &quot;នរណាម្នាក់&quot; ឬ &quot;អនាមិក&quot;"

msgid "Notify user(s) by email"
msgstr "ជូនដំណឹង​អ្នកប្រើ តាម​អ៊ីមែល"

msgid "Update"
msgstr "ធ្វើ​ឲ្យ​ទាន់សម័យ"

msgid "Add User"
msgstr "បន្ថែម​អ្នកប្រើ"

msgid "Search..."
msgstr "ស្វែងរក..."

msgid "Draft folder"
msgstr "ថត សេចក្ដី​ព្រាង"

msgid "Server replied: "
msgstr "ម៉ាស៊ីន​បម្រើ​បាន​ឆ្លើយតប ៖ "

msgid "Shares"
msgstr "ចែករំលែក"

#, php-format
msgid "Successfully changed permissions for folder %s"
msgstr "បាន​ផ្លាស់ប្ដូរ​សិទ្ធិ​សម្រាប់​ថត %s ដោយ​ជោគជ័យ"

#, php-format
msgid "Failed to change permissions for folder %s"
msgstr "បរាជ័យ​ក្នុងការ​ផ្លាស់ប្ដូរ​សិទ្ធិ​សម្រាប់​ថត %s"

#, php-format
msgid "No changes in permissions for folder %s"
msgstr "គ្មាន​ការ​ផ្លាស់ប្ដូរ​សិទ្ធិ​សម្រាប់​ថត %s ទេ"

msgid ""
"Cannot modify permissions on your own folder; you will always have full "
"rights on your folders."
msgstr "មិនអាច​កែប្រែ​សិទ្ធិ​នៅលើ​ថត​ផ្ទាល់ខ្លួន អ្នក​នឹង​មាន​សិទ្ធិ​ពេញលេញ​នៅ​លើ​ថត​របស់​អ្នក​ជានិច្ច ។"

#, php-format
msgid "No changes in permissions for user %s"
msgstr "គ្មាន​ការ​ផ្លាស់ប្ដូរ​សិទ្ធិ​សម្រាប់​​អ្នកប្រើ​ %s ទេ"

#, php-format
msgid "Successfully changed permissions for user %s."
msgstr "បាន​ផ្លាស់ប្ដូរ​សិទ្ធិ​សម្រាប់​អ្នកប្រើ %s ដោយ​ជោគជ័យ ។"

#, php-format
msgid "Failed to change permissions for user %s."
msgstr "បរាជ័យ​ក្នុង​ការ​ផ្លាស់ប្ដូរ​សិទ្ធិ​សម្រាប់​អ្នកប្រើ %s ។"

msgid "Specified user does not exist."
msgstr "មិនមាន​អ្នកប្រើ​ដែល​បាន​បញ្ជាក់​ទេ ។"

msgid "Anyone"
msgstr "នរណា​ម្នាក់"

msgid "Anonymous"
msgstr "អនាមិក"

#, php-format
msgid "Failed to send notification message to user %s"
msgstr "បរាជ័យ​ក្នុង​ការ​ផ្ញើ​សារ​ជូន​ដំណឹង​ទៅ​អ្នកប្រើ %s"

#, php-format
msgid "Failed to send notification message to users: %s"
msgstr "បរាជ័យ​ក្នុងការ​ផ្ញើ​សារ​ជូន​ដំណឹង​ទៅ​អ្នកប្រើ ៖ %s"

#, php-format
msgid "Successfully sent notification message to user %s"
msgstr "បាន​ផ្ញើ​សារ​ជូន​ដំណឹង​ទៅ​អ្នកប្រើ %s ដោយ​ជោគជ័យ"

#, php-format
msgid "Successfully sent notification message to users: %s"
msgstr "បាន​ផ្ញើ​សារ​ជូន​ដំណឹង​ទៅ​អ្នកប្រើ ៖ %s ដោយ​ជោគជ័យ"

msgid "User Permissions"
msgstr "សិទ្ធិ​អ្នកប្រើ"

#, php-format
msgid "Current Permissions for Folder: %s"
msgstr "សិទ្ធិ​បច្ចុប្បន្ន​សម្រាប់​ថត ៖ %s"

