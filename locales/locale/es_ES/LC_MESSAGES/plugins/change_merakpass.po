msgid "Submit"
msgstr "Enviar"

msgid "Cancel"
msgstr "Cancelar"

msgid "Cannot change password! (401)"
msgstr "No se puede cambiar la contraseña (401)"

msgid "Cannot change password! (Failure)"
msgstr "No se puede cambiar la contaseña! (Error)"

msgid "Change Password"
msgstr "Cambiar contraseña"

msgid "New Password"
msgstr "Contraseña nueva"

msgid "Old Password"
msgstr "Contraseña actual"

msgid "Options - Change Password"
msgstr "Opciones - Cambiar contraseña"

msgid "Password change cancelled."
msgstr "La acción ha sido cancelada."

msgid "Password changed successfully."
msgstr "Contraseña cambiada correctamente"

msgid "Please use your new password to log in from now on."
msgstr ""
"Su nueva contraseña ha sido activada, utilízela de ahora en adelante para "
"entrar. "

msgid "Secure"
msgstr "Seguro"

msgid "Use this to change your email password."
msgstr "Aquí se puede cambiar su contraseña."

msgid "Verify New Password"
msgstr "Repetir la contraseña nueva"

msgid "You must also type in your new password in the verify box."
msgstr "Repita la contraseña nueva para comprobarla."

msgid "You must type in a new password."
msgstr "Escriba una contraseña nueva."

msgid "You must type in your old password."
msgstr "Escriba su contraseña actual."

msgid "Your new password does not match the verify password."
msgstr ""
"Error. Su contraseña no coincide. Por favor, escriba nuevamente las "
"contraseñas."

msgid "Your new password must be different than your old password."
msgstr "Su nueva contraseña tiene que ser distinta a la contraseña actual."

msgid "Your old password is not correct."
msgstr "Su contraseña actual no es correcta."

msgid "Your password has NOT been changed."
msgstr "No se ha cambiado su contraseña."
