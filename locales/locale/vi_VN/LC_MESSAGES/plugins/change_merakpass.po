#: options.php:211
msgid "Submit"
msgstr "Gửi"

#: options.php:216
msgid "Cancel"
msgstr "Thôi"

#: options.php:331
msgid "Cannot change password! (401)"
msgstr "Không thể thay đổi mật khẩu. (401)"

#: options.php:411
msgid "Cannot change password! (Failure)"
msgstr "Không thể thay đổi mật khẩu. (Thất bại)"

#: functions.php:21
msgid "Change Password"
msgstr "Đổi mật khẩu"

#: options.php:201
msgid "New Password"
msgstr "Mật khẩu mới"

#: options.php:197
msgid "Old Password"
msgstr "Mật khẩu cũ"

#: options.php:165
msgid "Options - Change Password"
msgstr "Tùy chọn — Đổi mật khẩu"

#: functions.php:76
msgid "Password change cancelled."
msgstr "Việc thay đổi mật khẩu bị thôi."

#: functions.php:79
msgid "Password changed successfully."
msgstr "Mật khẩu đã được thay đổi."

#: functions.php:79
msgid "Please use your new password to log in from now on."
msgstr ""
"Hãy dùng mật khẩu mới để đăng nhập kể từ bây giờ."

#: options.php:165
msgid "Secure"
msgstr "Bảo mật"

#: functions.php:23
msgid "Use this to change your email password."
msgstr "Dùng cái này để thay đổi mật khẩu thư điện tử của bạn."

#: options.php:205
msgid "Verify New Password"
msgstr "Thẩm tra mật khẩu mới"

#: options.php:264
msgid "You must also type in your new password in the verify box."
msgstr "Bạn cũng cần phải gõ mật khẩu mới vào hộp xác nhận."

#: options.php:261
msgid "You must type in a new password."
msgstr "Bạn cần phải gõ một mật khẩu mới."

#: options.php:259
msgid "You must type in your old password."
msgstr "Bạn cần phải gõ mật khẩu cũ."

#: options.php:267
msgid "Your new password does not match the verify password."
msgstr "Mật khẩu mới không trùng với mật khẩu xác nhận."

#: options.php:271
msgid "Your new password must be different than your old password."
msgstr "Mật khẩu mới phải khác với mật khẩu cũ."

#: options.php:269
msgid "Your old password is not correct."
msgstr " Mật khẩu cũ không đúng."

#: functions.php:76
msgid "Your password has NOT been changed."
msgstr "Mật khẩu của bạn CHƯA thay đổi."
