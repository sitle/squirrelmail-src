#: options.php:211
msgid "Submit"
msgstr "Send"

#: options.php:216
msgid "Cancel"
msgstr "Avbryt"

#: options.php:331
msgid "Cannot change password! (401)"
msgstr "Kan ikkje endra passordet! (401)"

#: options.php:411
msgid "Cannot change password! (Failure)"
msgstr "Kan ikkje endra passordet! (Feilet)"

#: functions.php:21
msgid "Change Password"
msgstr "Endra passord"

#: options.php:201
msgid "New Password"
msgstr "Nytt passord"

#: options.php:197
msgid "Old Password"
msgstr "Gammalt passord"

#: options.php:165
msgid "Options - Change Password"
msgstr "Val - Endra passord"

#: functions.php:76
msgid "Password change cancelled."
msgstr "Passord endring avbrutt."

#: functions.php:79
msgid "Password changed successfully."
msgstr "Vellukka endring av passord."

#: functions.php:79
msgid "Please use your new password to log in from now on."
msgstr "Bruk ditt nye passord for å logg inn frå nå av."

#: options.php:165
msgid "Secure"
msgstr "Trygg"

#: functions.php:23
msgid "Use this to change your email password."
msgstr "Bruk dette for å endra e-post passordet."

#: options.php:205
msgid "Verify New Password"
msgstr "Stadfest nytt passord"

#: options.php:264
msgid "You must also type in your new password in the verify box."
msgstr "Må også skriva inn nytt passord i kontroll boksen."

#: options.php:261
msgid "You must type in a new password."
msgstr "Skriv inn eit nytt passord."

#: options.php:259
msgid "You must type in your old password."
msgstr "Skriv inn gammalt passord."

#: options.php:267
msgid "Your new password does not match the verify password."
msgstr "Nytt passord og kontroll passord er ikkje like."

#: options.php:271
msgid "Your new password must be different than your old password."
msgstr "Nytt passord må vera forskjelleg frå gammalt passord."

#: options.php:269
msgid "Your old password is not correct."
msgstr "Gammalt passord er feil."

#: functions.php:76
msgid "Your password has NOT been changed."
msgstr "Passord er ikkje endra."
