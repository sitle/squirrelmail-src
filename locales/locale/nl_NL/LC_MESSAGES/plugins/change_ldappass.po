msgid "LDAP bind failed."
msgstr "LDAP-bind is mislukt."

#: functions.php:127 functions.php:153
#, php-format
msgid "Error: %s"
msgstr "Fout: %s"

#: functions.php:152
msgid "LDAP search failed."
msgstr "LDAP-zoeken is mislukt."

