#: setup.php:208
msgid "Password change cancelled."
msgstr "Passord endring annullert."

#: setup.php:208
msgid "Your password has NOT been changed."
msgstr "Passordet ditt er IKKJE endra."

#: setup.php:211
msgid "Password changed successfully."
msgstr "Vellukka passord endring."

#: setup.php:208
msgid "Please use your new password to log in from now on."
msgstr "Bruk nytt passord til innlogging frå nå av."

#: options.php:143
msgid "Change Password"
msgstr "Endra passord"

#: options.php:143
msgid "Secure"
msgstr "Trygg"

#: options.php:170
msgid "Old Password"
msgstr "Gammalt passord"

#: options.php:174
msgid "New Password"
msgstr "Nytt passord"

#: options.php:178
msgid "Verify New Password"
msgstr "Stadfest nytt passord"

#: options.php:184
msgid "Submit"
msgstr "Send"

#: options.php:189
msgid "Cancel"
msgstr "Avbryt"

#: options.php:227
msgid "You must type in your old password."
msgstr "skriv inn gammalt passord."

#: options.php:229
msgid "You must type in a new password."
msgstr "Skriv in nytt passord."

#: options.php:232
msgid "You must also type in your new password in the verify box."
msgstr "Skriv inn nytt passord i kontroll boksen."

#: options.php:235
msgid "Your new password does not match the verify password."
msgstr "Nytt passord og kontroll passord er ikkje like."

#: options.php:237
msgid "Your old password is not correct."
msgstr "Gammalt passord er ikkje korrekt."

#: options.php:240
msgid "Your new password must be different than your old password."
msgstr "Nytt passord må vera forskjelleg frå gammalt passord."

#: options.php:294
msgid "Cannot connect to Database Server, please try later!"
msgstr "Kan ikkje kopla til database tenar, prøv seinare!"

#: options.php:299
msgid "Database not found on server"
msgstr "Database fins ikkje på tenar"

#: options.php:364
msgid "SQL call failed, try again later."
msgstr "SQL misslukka, prøv seinare."

#: options.php:379 options.php:386
msgid "Strange, your old password does not match the database... rejecting."
msgstr "Merkeleg, gammalt passord passar ikkje med databasen....forkast"

#: options.php:391
msgid "Duplicate login entries detected, cannot change password!"
msgstr "Fant dupliserte innloggings oppføringar, kan ikkje endra passordet!"

#: options.php:468
msgid "Password change was not successful!"
msgstr "Passord endringa feilet!"
