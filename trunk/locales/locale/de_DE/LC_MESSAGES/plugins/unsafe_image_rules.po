msgid "Cc"
msgstr "Cc"

#: /usr/share/smtest/plugins/unsafe_image_rules/options.php:227
msgid "To or Cc"
msgstr "An oder Cc"

#: /usr/share/smtest/plugins/unsafe_image_rules/options.php:230
msgid "Subject"
msgstr "Betreff"

#: /usr/share/smtest/plugins/unsafe_image_rules/options.php:249
msgid "NB"
msgstr "P.S."

#: /usr/share/smtest/plugins/unsafe_image_rules/options.php:251
msgid "When specifying a Reg.Exp you must include delimeters."
msgstr "Wenn Sie einen Reg.Ausdruck angeben, dann müssen Sie "
"Begrenzungszeichen angeben."

#: /usr/share/smtest/plugins/unsafe_image_rules/options.php:252
msgid "The 'Contains' check will be done with case-insensitive match."
msgstr "Die 'enthält' Prüfung wird ohne Beachtung der Groß-/Klein-"
"Schreibung durchgeführt."

