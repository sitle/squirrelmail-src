msgid "Change Password"
msgstr "Jelszóváltoztatás"

#: options.php:77
msgid "Closing Connection"
msgstr "Kapcsolat lezárása"

#: options.php:137
msgid "Connecting to Password Server"
msgstr "Kapcsolódás a levelezőszerverhez"

#: options.php:141
msgid "ERROR:"
msgstr "A következő hiba történt:"

#: options.php:52
msgid "New Password"
