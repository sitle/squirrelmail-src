# of =ferstjoere?
msgid "Submit"
msgstr "Yntsjinje"

msgid "Cancel"
msgstr "Annulearje"

msgid "Cannot change password! (401)"
msgstr "Kin wachtwurd net wizigje! (401)"

msgid "Cannot change password! (Failure)"
msgstr "Kin wachtwurd net wizigje! (Fout)"

msgid "Change Password"
msgstr "Wachtwurd wizigje"

msgid "New Password"
msgstr "Nij wachtwurd"

msgid "Old Password"
msgstr "Alde wachtwurd"

msgid "Options - Change Password"
msgstr "Opsjes - wachtwurd wizigje"

msgid "Password change cancelled."
msgstr "Wizigjen fan wachtwurd annulearre."

msgid "Password changed successfully."
msgstr "Wachtwurd is wizige."

msgid "Please use your new password to log in from now on."
msgstr "Brûk fan no ôf jo nije wachtwurd om yn te loggen."

msgid "Secure"
msgstr "Feilich"

msgid "Use this to change your email password."
msgstr "Brûk dit om jo e-mailwachtwurd te wizigjen"

# verify=ferifiearje?
msgid "Verify New Password"
msgstr "Nij wachtwurd befêstigje"

msgid "You must also type in your new password in the verify box."
msgstr "Jo moatte jo nije wachtwurd ek yn it ferifikaasjefak yntype."

msgid "You must type in a new password."
msgstr "Jo moatte in nij wachtwurd yntype."

msgid "You must type in your old password."
msgstr "Jo moatte jo âlde wachtwurd yntype"

msgid "Your new password does not match the verify password."
msgstr "De beide nije wachtwurden komme net oerien."

msgid "Your new password must be different than your old password."
msgstr "Jo nije wachtwurd mei net itselde wêze as jo âlde wachtwurd."

msgid "Your old password is not correct."
msgstr "Jo âlde wachtwurd is ferkeard."

msgid "Your password has NOT been changed."
msgstr "Jo wachtwurd is NET wizige."

