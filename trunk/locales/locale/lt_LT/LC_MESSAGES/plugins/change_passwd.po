#: functions.php:39
#: options.php:274
msgid "Change Password"
msgstr "Pakeisti slaptažodį"

#: functions.php:41
msgid "Use this to change your email password."
msgstr "Čia galima pakeisti pašto slaptažodį."

#: options.php:80
msgid "Could not determine your username."
msgstr "Nepavyko nustatyti Jūsų vartotojo vardo."

#: options.php:81
#: options.php:245
msgid "Please contact your system administrator."
msgstr "Susisiekite su sistemos administratoriumi."

#: options.php:86
msgid "Please enter your current password."
msgstr "Įveskite Jūsų dabartinį slaptažodį."

#: options.php:91
msgid "Please enter your new password."
msgstr "Įveskite naują slaptažodį."

#: options.php:96
msgid "Please re-enter your new password."
msgstr "Pakartokite naujo slaptažodžio įvedimą."

#: options.php:105
#: options.php:293
#, c-format
msgid "Your new password must be at least %s characters long."
msgstr "Jūsų naujasis slaptažodis turi turėti bent %s simbolius."

#: options.php:117
msgid "The confirmation of your new password did not match."
msgstr "Nesutapo įvesti naujieji slaptažodžiai."

#: options.php:142
msgid "Your current password is not correct."
msgstr "Nurodytas senasis slaptažodis yra neteisingas."

#: options.php:213
msgid "Your password was changed successfully."
msgstr "Jūsų slaptažodis buvo sėkmingai pakeistas."

#: options.php:227
msgid "The password was modified successfully"
msgstr "Slaptažodis buvo sėkmingai pakeistas"

#: options.php:228
msgid "Missing new password"
msgstr "Nenurodytas naujas slaptažodis"

#: options.php:229
msgid "Missing current password"
msgstr "Nenurodytas dabartinis slaptažodis"

#: options.php:230
#, c-format
msgid "The password for this user cannot be changed due to security constraints: %s"
msgstr "Šio vartotojo slaptažodis negali būti pakeistas dėl nustatytų saugumo apribojimų: %s"

#: options.php:231
msgid "The new password is equal to the current password. Choose another password."
msgstr "Naujasis slaptažodis sutampa su anksčiau naudotu. Pasirinkite kitą slaptažodį."

#: options.php:232
#: options.php:233
msgid "Could not read password file"
msgstr "Nepavyko perskaityti slaptažodžių bylos"

#: options.php:234
msgid "Temporary file could not be opened"
msgstr "Nepavyko atidaryti laikinos bylos"

#: options.php:235
msgid "Current password is incorrect"
msgstr "Nurodytas slaptažodis neteisingas"

#: options.php:236
#, c-format
msgid "User does not exist: %s"
msgstr "Tokio vartotojo nėra: %s"

#: options.php:237
msgid "Virtual memory exhausted"
msgstr "Operacijai pritrūko atminties"

#: options.php:238
msgid "Missing username"
msgstr "Trūksta vartotojo vardo"

#: options.php:244
msgid "An error has occurred while attempting to change your password."
msgstr "Bandant pakeisti Jūsų slaptažodį įvyko klaida."

#: options.php:253
msgid "Command output:"
msgstr "Komanda išvedė:"

#: options.php:258
msgid "Return code:"
msgstr "Atsakymas:"

#: options.php:284
msgid "Please read the following instructions before editing your password:"
msgstr "Perskaitykite žemiau esančias instrukcijas prieš keisdami Jūsų slaptažodį:"

#: options.php:287
msgid "You may use letters, numbers, and other special characters on your keyboard."
msgstr "Jūs galite naudoti raides, skaičius ir specialius simbolius, esančius ant klaviatūros."

#: options.php:289
msgid "Passwords are case-sensititve, so an 'A' is not the same as an 'a'."
msgstr "Slaptažodžiai priklauso nuo raidžių registro. A didžioji yra visai kitas simbolis nei a mažoji."

#: options.php:310
msgid "Your user name:"
msgstr "Jūsų vartotojo vardas:"

#: options.php:317
msgid "Current password:"
msgstr "Dabartinis slaptažodis:"

#: options.php:322
msgid "New password:"
msgstr "Naujas slaptažodis:"

#: options.php:327
msgid "Confirm new password:"
msgstr "Patvirtinkite naują slaptažodį:"

