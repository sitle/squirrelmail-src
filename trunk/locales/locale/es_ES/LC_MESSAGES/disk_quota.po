msgstr "Espacio_de_Disco_en_Uso"

msgid "Details"
msgstr "Detalles"

msgid "You_have_no_quota_limit"
msgstr "Cuota_ilimitada"

msgid "Please_ignore_the_following_information"
msgstr "No_en_cuenta_la_siguiente_informacion"

