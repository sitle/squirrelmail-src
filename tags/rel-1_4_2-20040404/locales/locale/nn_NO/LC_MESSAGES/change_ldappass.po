#: options.php:94 setup.php:17
msgid "Change Password"
msgstr "Endra passord"

#: options.php:109
msgid "Old Password:"
msgstr "Gammalt passord"

#: options.php:113
msgid "New Password:"
msgstr "Nytt passord"

#: options.php:117
msgid "Verify New Password:"
msgstr "Stadfest nytt passord"

#: options.php:139
msgid "You must type in your old password."
msgstr "Skriv inn gammalt passord."

#: options.php:140
msgid "You must type in a new password."
msgstr "Skriv inn nytt passord."

#: options.php:141
msgid "You must also type in your new password in the verify box."
msgstr "Skriv inn nytt passord i kontrollfeltet."

#: options.php:142
msgid "Your new password doesn't match the verify password."
msgstr "Nytt passord og kontrollfeltet stemmer ikkje overens"

#: options.php:159
msgid "Connecting to LDAP Server"
msgstr "Koplar til LDAP tenar"

#: options.php:163
msgid "Can't connect to Directory Server, please try later!"
msgstr "Klarer ikkje å kople til katalogtenar, prøv seinare!"

#: options.php:170
msgid "Duplicate login entries detected, cannot change password!"
msgstr "Fant dupliserte innlogging oppføringar, kan ikkje endra passordet!"

#: options.php:174
msgid ""
"Your login account was not found in the LDAP database, cannot change "
"password!"
msgstr "Fant ikkje konto i LDAP databasen, kan ikkje endra passordet!"

#: options.php:184 options.php:247
msgid "Your old password is not correct."
msgstr "Gamalt passord er ikkje rett."

#: options.php:187
msgid "Bind to database as $dn successful."
msgstr "Vellukka tilkopling til database som $dn."

#: options.php:191
msgid ""
"Could not bind to the LDAP server as $ldap_manager_dn. Is the password "
"correct?"
msgstr "Kunne ikkje kople til LDAP tenaren som $ldap_manager_dn, Er passord rett?"

#: options.php:194
msgid "Bind to database as $ldap_manager_dn successful."
msgstr "Vellukka tilkopling til database som $ldap_manager_dn "

#: options.php:223
msgid "Password type is {crypt}, sub-type $stype"
msgstr "Passord typen er {crypt}, sub-type $stype"

#: options.php:227
msgid "Password type is {MD5}"
msgstr "Passord type er {MD5}"

#: options.php:231
msgid "Password type is {SHA}"
msgstr "Passord type er {SHA}"

#: options.php:235
msgid "Password type is {SMD5}"
msgstr "Passord type er {SMD5}"

#: options.php:248
msgid "Stored Password: $lpass  Old Password: $cpass"
msgstr "Lagre passord : $lpass  Gamalt passord: $cpass"

#: options.php:271
msgid "New Password: $newpass"
msgstr "Nytt passord: $newpass"

#: options.php:280
msgid "Password changed for user $username"
msgstr "Passord endra for brukar $username"

#: options.php:294
msgid "Password changed successfully"
msgstr "Passordendringa var vellukka."

#: options.php:302
msgid "SMB Password change was not successful, so LDAP not changed!"
msgstr "SMB passord endringa feila, så LDAP ikkje endra!"

#: options.php:306
msgid "LDAP Password change was not successful!"
msgstr "LDAP passord endringa feilet!"

#: setup.php:19
msgid ""
"This connects to your local Password Server to change your login password "
"for all services."
msgstr "Kopler til lokal passord tenar som endrar innloggingspassordet for alle tjenestene."

