      &#1054;&#1073;&#1080;&#1076;&#1074;&#1072; &#1094;&#1110; &#1087;&#1086;&#1089;&#1080;&#1083;&#1072;&#1085;&#1085;&#1103; &#1084;&#1086;&#1078;&#1091;&#1090;&#1100; &#1073;&#1091;&#1090;&#1080; &#1085;&#1077;&#1076;&#1110;&#1081;&#1089;&#1085;&#1110; &#1103;&#1082;&#1097;&#1086; &#1087;&#1086;&#1090;&#1086;&#1095;&#1085;&#1077; &#1087;&#1086;&#1074;&#1110;&#1076;&#1086;&#1084;&#1083;&#1077;&#1085;&#1085;&#1103;
      &#1108; &#1085;&#1072;&#1081;&#1087;&#1077;&#1088;&#1096;&#1080;&#1084;, &#1085;&#1072;&#1081;&#1086;&#1089;&#1090;&#1072;&#1085;&#1085;&#1110;&#1096;&#1080;&#1084;, &#1072;&#1073;&#1086; &#1074;&#1079;&#1072;&#1075;&#1072;&#1083;&#1110; &#1108;&#1076;&#1080;&#1085;&#1080;&#1084; &#1091;
      &#1074;&#1089;&#1110;&#1081; &#1087;&#1086;&#1096;&#1090;&#1086;&#1074;&#1110;&#1081; &#1090;&#1077;&#1094;&#1110;. &#1053;&#1072;&#1090;&#1080;&#1089;&#1085;&#1077;&#1085;&#1085;&#1103; &#1085;&#1072; &#1094;&#1110; &#1087;&#1086;&#1089;&#1080;&#1083;&#1072;&#1085;&#1085;&#1103; &#1074;&#1080;&#1074;&#1077;&#1076;&#1077; &#1085;&#1072; &#1077;&#1082;&#1088;&#1072;&#1085; &#1074;&#1110;&#1076;&#1087;&#1086;&#1074;&#1110;&#1076;&#1085;&#1086;
      &#1085;&#1072;&#1089;&#1090;&#1091;&#1087;&#1085;&#1077; &#1072;&#1073;&#1086; &#1087;&#1086;&#1087;&#1077;&#1088;&#1077;&#1076;&#1085;&#1108; &#1087;&#1086;&#1074;&#1110;&#1076;&#1086;&#1084;&#1083;&#1077;&#1085;&#1085;&#1103;.
