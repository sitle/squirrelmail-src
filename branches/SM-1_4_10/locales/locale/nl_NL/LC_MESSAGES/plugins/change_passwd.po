#: functions.php:39 options.php:274
msgid "Change Password"
msgstr "Wachtwoord wijzigen"

#: functions.php:41
msgid "Use this to change your email password."
msgstr "Gebruik dit om uw wachtwoord te wijzigen."

#: options.php:80
msgid "Could not determine your username."
msgstr "Kon uw gebruikersnaam niet achterhalen."

#: options.php:81 options.php:245
msgid "Please contact your system administrator."
msgstr "Neem contact op met uw systeembeheerder."

#: options.php:86
msgid "Please enter your current password."
msgstr "Voer uw huidige wachtwoord in."

#: options.php:91
msgid "Please enter your new password."
msgstr "Voer uw nieuwe wachtwoord in."

#: options.php:96
msgid "Please re-enter your new password."
msgstr "Voer uw nieuwe wachtwoord nogmaals in."

#: options.php:105 options.php:293
#, c-format
msgid "Your new password must be at least %s characters long."
msgstr "Uw nieuwe wachtwoord dient tenminste %s lettertekens te bevatten."

#: options.php:117
msgid "The confirmation of your new password did not match."
msgstr "De bevestiging van uw nieuwe wachtwoord kwam niet overeen."

#: options.php:142
msgid "Your current password is not correct."
msgstr "Uw huidige wachtwoord is onjuist."

#: options.php:213
msgid "Your password was changed successfully."
msgstr "Uw wachtwoord is met succes gewijzigd."

#: options.php:227
msgid "The password was modified successfully"
msgstr "Het wachtwoord is met succes gewijzigd"

#: options.php:228
msgid "Missing new password"
msgstr "Nieuw wachtwoord ontbreekt"

#: options.php:229
msgid "Missing current password"
msgstr "Huidig wachtwoord ontbreekt"

#: options.php:230
#, c-format
msgid "The password for this user cannot be changed due to security constraints: %s"
msgstr "Het wachtwoord voor deze gebruiker kan niet worden gewijzigd vanwege beveiligingsbeperkingen: %s"

#: options.php:231
msgid "The new password is equal to the current password. Choose another password."
msgstr "Het nieuwe wachtwoord is gelijk aan het oude wachtwoord. Kies een ander wachtwoord."

#: options.php:232 options.php:233
msgid "Could not read password file"
msgstr "Kon het wachtwoordenbestand niet lezen"

#: options.php:234
msgid "Temporary file could not be opened"
msgstr "Kon het tijdelijke bestand niet openen"

#: options.php:235
msgid "Current password is incorrect"
msgstr "Huidige wachtwoord is onjuist"

#: options.php:236
#, c-format
msgid "User does not exist: %s"
msgstr "Gebruiker bestaat niet: %s"

#: options.php:237
msgid "Virtual memory exhausted"
msgstr "Onvoldoende virtueel geheugen"

#: options.php:238
msgid "Missing username"
msgstr "Gebruikersnaam ontbreekt"

#: options.php:244
msgid "An error has occurred while attempting to change your password."
msgstr "Er deed zich een fout voor tijdens het wijzigen van uw wachtwoord."

#: options.php:253
msgid "Command output:"
msgstr "Commandouitvoer:"

#: options.php:258
msgid "Return code:"
msgstr "Teruggegeven code:"

#: options.php:284
msgid "Please read the following instructions before editing your password:"
msgstr "Lees de volgende instructies voordat u uw wachtwoord wijzigt:"

#: options.php:287
msgid "You may use letters, numbers, and other special characters on your keyboard."
msgstr "U kunt gebruik maken van letters, cijfers en andere speciale tekens op uw toetsenbord."

#: options.php:289
msgid "Passwords are case-sensititve, so an 'A' is not the same as an 'a'."
msgstr "Wachtwoorden zijn hoofdlettergevoelig, dus een 'A' is niet hetzelfde als een 'a'."

#: options.php:310
msgid "Your user name:"
msgstr "Uw gebruikersnaam:"

#: options.php:317
msgid "Current password:"
msgstr "Huidig wachtwoord:"

#: options.php:322
msgid "New password:"
msgstr "Nieuw wachtwoord:"

#: options.php:327
msgid "Confirm new password:"
msgstr "Nieuw wachtwoord bevestigen:"

